	ВыбранноеПоле = Группировка.Выбор.Элементы.Добавить(Тип(""ВыбранноеПолеКомпоновкиДанных""));
	ВыбранноеПоле.Поле = Новый ПолеКомпоновкиДанных(ИмяПоля);
	ВыбранноеПоле.Заголовок = ИмяПоля;

